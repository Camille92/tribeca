{
  "name": "tribeca",
  "version": "2.0.0",
  "description": "A high frequency, market making cryptocurrency trading platform",
  "dependencies": {
    "@angular/common": "^2.4.3",
    "@angular/compiler": "^2.4.3",
    "@angular/compiler-cli": "^2.4.3",
    "@angular/core": "^2.4.3",
    "@angular/forms": "^2.4.3",
    "@angular/http": "^2.4.3",
    "@angular/platform-browser": "^2.4.3",
    "@angular/platform-browser-dynamic": "^2.4.3",
    "@angular/router": "^3.4.3",
    "@types/angular": "^1.6.1",
    "@types/bunyan": "0.0.35",
    "@types/express": "^4.0.34",
    "@types/express-serve-static-core": "^4.0.40",
    "@types/jquery": "^2.0.39",
    "@types/lodash": "^4.14.50",
    "@types/marked": "0.0.28",
    "@types/node": "^7.0.0",
    "@types/q": "0.0.32",
    "@types/request": "^0.0.39",
    "@types/socket.io": "^1.4.27",
    "@types/socket.io-client": "^1.4.29",
    "@types/ws": "0.0.37",
    "ag-grid": "^7.1.0",
    "ag-grid-ng2": "^7.1.2",
    "agentkeepalive": "^3.0.0",
    "basic-auth": "^1.1.0",
    "basic-auth-connect": "1.0.0",
    "body-parser": "^1.15.2",
    "browserify": "^13.3.0",
    "bunyan": "^1.8.5",
    "collections": "^5.0.6",
    "compression": "^1.6.2",
    "connect": "^3.5.0",
    "core-js": "^2.4.1",
    "es6-shim": "^0.35.2",
    "express": "^4.14.0",
    "forever": "^0.15.3",
    "heapdump": "^0.3.7",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "lynx": "^0.2.0",
    "marked": "0.3.6",
    "moment": "^2.17.1",
    "mongodb": "^2.2.21",
    "ng2-popover": "0.0.13",
    "node-uuid": "1.4.7",
    "q": "1.4.1",
    "reflect-metadata": "^0.1.9",
    "request": "^2.79.0",
    "rxjs": "^5.0.3",
    "shortid": "^2.2.6",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "ts-helpers": "^1.1.2",
    "typescript": "^2.1.5",
    "ws": "^1.1.1",
    "zone.js": "^0.7.5"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.37",
    "codacy-coverage": "^2.0.0",
    "istanbul": "^1.0.0-alpha",
    "istanbul-coveralls": "^1.0.3",
    "mocha": "^3.2.0",
    "ts-node": "^2.0.0"
  },
  "main": "./tribeca.js",
  "scripts": {
    "log": "bunyan log/tribeca.log 2>/dev/null",
    "test": "mocha --compilers ts:ts-node/register test/*.ts",
    "test-cov": "ts-node ./node_modules/istanbul/lib/cli.js cover --report lcovonly --dir test/coverage -e .ts _mocha -- --compilers ts:ts-node/register test/*.ts",
    "send-cov": "cd test && cat coverage/lcov.info | codacy-coverage && istanbul-coveralls",
    "postinstall": "npm run compile-service && npm run compile-admin && npm run compile-static && npm run compile-bundle",
    "compile-service": "tsc --alwaysStrict -t ES6 -m commonjs --outDir app src/service/*.ts src/service/*/*.ts src/common/*.ts",
    "compile-admin": "tsc --alwaysStrict --experimentalDecorators -t ES6 -m commonjs --outDir app/service/admin/js src/admin/*.ts src/common/*.ts",
    "compile-static": "cp -R src/static/* app/service/admin/",
    "compile-bundle": "browserify app/service/admin/js/admin/client.js -o app/service/admin/js/admin/bundle.min.js",
    "restart": "npm stop && npm start",
    "stop": "forever stop -a -l /dev/null tribeca.js || :",
    "start": "npm run test-compile && forever start -a -l /dev/null tribeca.js",
    "test-compile": "test -d app || npm $(test -d node_modules && echo run post)install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ctubio/tribeca.git"
  },
  "author": "Michael Grosner",
  "contributors": [
    {
      "name": "Carles Tubio"
    }
  ],
  "license": "ISC"
}
